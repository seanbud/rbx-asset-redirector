{
	"manifest_version": 3,
	"name": "Roblox Asset Redirector",
	"version": "1.1",
	"description": "Quickly redirect to Roblox assets by ID or Link.",
	"icons": {
		"128": "icon.svg"
	},
	"action": {
		"default_icon": {
			"128": "icon.svg"
		},
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"scripting"
	]
}